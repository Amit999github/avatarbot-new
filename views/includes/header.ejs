<div class="sidebar">
    <a href="#" class="logo">
        <i class='bx bxs-zap'></i>
        <div class="logo-name"><span>Avatar</span>Bot</div>
    </a>
    <ul class="side-menu">
        <li class=""><a href="/" class="sidebar-link" data-page="/dashboard"><i class='bx bxs-dashboard'></i>Dashboard</a></li>
        <% for (const room of rooms) { %>
        <li class=""><a href="/listings/<%= room.roomName %>" class="sidebar-link" data-page="/<%= room.roomName %>"><i class='bx bxs-dashboard'></i><%= room.roomName %></a></li>
        <% } %>
        <li><a href="#" class="sidebar-link add-room" data-page="page6"><i class='bx bx-plus'></i>Add Rooms</a></li>
    </ul>
    <ul class="side-menu">
        <li>
            <a class="logout">
                <form action="/auth/signout" method="POST">
                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                <button type="submit"><i class='bx bx-log-out-circle'></i>Sign Out</button>
                </form>                  
            </a>
        </li>
    </ul>
</div>    
    <!-- Navbar -->
<div class="content">
    <nav>
        <i class='bx bx-menu'></i>
        <form action="#">
            <div class="form-input">
                <input type="search" placeholder="Search...">
                <button class="search-btn" type="submit"><i class='bx bx-search'></i></button>
            </div>
        </form>
        <p id="user_icon" class="user_id"><i class='bx bx-user'></i></p>
        <span  id="mail_box" class="mail ">N/A</span>
        <input type="checkbox" id="theme-toggle" hidden>
        <label for="theme-toggle" class="theme-toggle"><i class="fas" id="theme-icon"></i></label>
        <a href="#" class="notif">
            <i class='bx bx-bell'></i>
            <span class="count">12</span>
        </a>
        <a href="#" class="profile">
            <img src="/images/sub_logo.png">    
        </a>
    </nav>
